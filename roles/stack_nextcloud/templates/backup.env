PASSPHRASE={{ nextcloud_backup_passphrase }}

TASK_dump_PERIODICITY=daily
TASK_dump_PREPARE=mkdir -p /mnt/backup/nextcloud_dump
TASK_dump_EXTRACT=mysqldump --host=$MYSQL_HOST --user=$MYSQL_USER --password=$MYSQL_PASSWORD $MYSQL_DATABASE > $SRC/nextcloud-database-dump.sql
TASK_dump_LOAD=duplicity full --force $SRC file:///mnt/backup/nextcloud_dump
